<%- include('partials/header'); -%>

<h1>Home</h1>

<p> <%= startingcontent %> </p>


<% newPosts.map(post => { %>
    <form action="/delete" method="POST">
     
    <h1><%= post.title %></h1>
    <p><%= post.content.substring(0, 100) + '.....' %>
    <a href="/posts/<%=post._id%>">READ MORE</a> 
      <button type="submit" name="submit" value="<%=post._id%>">Delete</button>
    </p> 
    </form>
<% })%>












